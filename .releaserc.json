{
  "branch": "main",
  "plugins": [
    [
      "@semantic-release/commit-analyzer",
      {
        "preset": "angular",
        "releaseRules": [
          {"breaking": true, "release": "minor"},
          {"tag": "Breaking", "release": "minor"},
          {"message": "/^Merge pull request/", "release": "patch"}
        ]
      }
    ],
    "@semantic-release/release-notes-generator",
    "@semantic-release/github"
  ]
}
